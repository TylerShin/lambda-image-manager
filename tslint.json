
{
  "extends": "tslint:recommended",
  "rulesDirectory": ["node_modules/tslint-microsoft-contrib"],
    "rules": {
    "jsdoc-format": false,
    "no-string-literal": false,
    "object-literal-sort-keys": false,
    "variable-name": false,
    "no-console": false,
    "indent": ["spaces"],
    "no-var-requires": false,
    "ordered-imports": [false],
    "max-line-length": [
      false
    ]
  }
}
